{
	"name": "thesis-unit-converter",
	"displayName": "thesis-unit-converter",
	"description": "Convert px to thesis units or rem",
	"version": "0.0.3",
	"publisher": "v-toro",
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Formatters"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Vane-Toro/thesis-unit-converter"
	},
	"activationEvents": [
		"onCommand:extension.pxToThesisUnit"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.pxToThesisUnit",
				"title": "thesis converter"
			},
			{
				"command": "extension.pxToRem",
				"title": "thesis converter"
			}
		],
		"keybindings": [
			{
				"command": "extension.pxToThesisUnit",
				"key": "Alt+z"
			},
			{
				"command": "extension.pxToRem",
				"key": "Alt+s"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.43.0",
		"eslint": "^6.8.0",
		"eslint-config-airbnb-base": "^14.1.0",
		"eslint-config-google": "^0.14.0",
		"eslint-plugin-import": "^2.20.1",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}
